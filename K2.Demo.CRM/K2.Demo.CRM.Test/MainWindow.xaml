        <Button Content="Add Doc" Height="23" HorizontalAlignment="Left" Margin="416,12,0,0" Name="btnAddDocLocation" VerticalAlignment="Top" Width="75" Click="btnAddDocLocation_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="20,12,0,0" Name="txtEntityId" VerticalAlignment="Top" Width="279" Text="Entity Id" />
