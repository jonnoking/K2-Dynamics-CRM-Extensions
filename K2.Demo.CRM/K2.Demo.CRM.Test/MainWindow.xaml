        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,107,0,0" Name="cbEntities" VerticalAlignment="Top" Width="287" DisplayMemberPath="DisplayName" />
        <Button Content="Get Entities" Height="23" HorizontalAlignment="Left" Margin="416,107,0,0" Name="btnGetEntities" VerticalAlignment="Top" Width="75" Click="btnGetEntities_Click" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,155,0,0" Name="cbAttributes" VerticalAlignment="Top" Width="287" DisplayMemberPath="DisplayName" />
