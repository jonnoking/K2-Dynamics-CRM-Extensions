        <Button Content="Get Picklist" Height="23" HorizontalAlignment="Left" Margin="416,203,0,0" Name="btnGetPicklist" VerticalAlignment="Top" Width="75" Click="btnGetPicklist_Click" />
        <Button Content="Get Rest" Height="23" HorizontalAlignment="Left" Margin="416,60,0,0" Name="btnGetRESTEntities" VerticalAlignment="Top" Width="75" Click="btnGetRESTEntities_Click" />
        <Button Content="Generate Process" Height="23" HorizontalAlignment="Left" Margin="416,306,0,0" Name="btnGenerateCRMProcess" VerticalAlignment="Top" Width="104" Click="btnGenerateCRMProcess_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,307,0,0" Name="txtProcessPath" VerticalAlignment="Top" Width="287" Text="c:\K2 Solutions\genproc\" />
